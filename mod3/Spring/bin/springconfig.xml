<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
     http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context-4.0.xsd">
     <bean id="employee1" class="com.cg.lab1.prob2.Employeebean">
     <property name="employeeId" value="12345"/>
     <property name="employeeName" value="Harriet"/>
     <property name="salary" value="40000.0"/>
     <property name="age" value="30"/>
     <property name="businessUnit" ref="sbu"/>
     </bean>
     <bean id="sbu" class="com.cg.lab1.prob2.SbuBean">
     <property name="subId" value="PES-BU"/>
     <property name="subName" value="Product Engineering Services"/>
     <property name="subHead" value="Kiran Rao"/>
     </bean>
     </beans>
     